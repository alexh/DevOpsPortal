@model DevOpsPortal.Models.CodeDeployRequestViewModel

@{
    ViewBag.Title = "App Deploy Request";
}
<link rel="stylesheet" href="~/styles/normalize.css" />
<link rel="stylesheet" href="~/styles/InputField.css" />
<link href="~/styles/FormStyle.css" rel="stylesheet" />
<link href="~/styles/DatePicker/bootstrap-datepicker3.css" rel="stylesheet" />
<link href="~/styles/DatePicker/bootstrap-datepicker3.standalone.min.css" rel="stylesheet" />



<!--BEGIN CONTENT-->


<div class="panel panel-lightblue" style="margin: 1%">
    <div class="panel-heading">
        Application Only Deployment Request Form
    </div>
    <div class="panel-body pan">
        @using (Html.BeginForm()) {
            @Html.AntiForgeryToken()

            <div class="form-body pal">

                @Html.Partial("CodeDeployRequestPartial", Model)

            </div>
            <div class="form-actions text-right pal">
                <button type="submit" class="btn btn-lightblue">
                    Submit
                </button>
            </div>
        }
    </div>
</div>




