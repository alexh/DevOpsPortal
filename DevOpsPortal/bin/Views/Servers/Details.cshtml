@model DevOpsPortal.Models.Server

@{
    ViewBag.Title = "Details";
}


<!DOCTYPE html>
<html lang="en">
<head>
    <title>Details</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="~/images/icons/favicon.ico">
    <link rel="apple-touch-icon" href="~/images/icons/favicon.png">
    <link rel="apple-touch-icon" sizes="72x72" href="~/images/icons/favicon-72x72.png">
    <link rel="apple-touch-icon" sizes="114x114" href="~/images/icons/favicon-114x114.png">
    <link type="text/css" rel="stylesheet" href="~/styles/main.css">
</head>
<body>
    <div>

        <div id="page-wrapper">
            <!--BEGIN CONTENT-->
            <div class="page-content">
                <div id="tab-general">


                </div>
                <div class="row mbl">
                    <h2>
                        @Html.DisplayFor(model => model.ComputerName)
                        - Details
                    </h2>
                    <div class="col-lg-12">
                        <hr />
                        <dl class="dl-horizontal">
                            <dt id="details">
                                @Html.DisplayNameFor(model => model.ComputerName)
                            </dt>

                            <dd id="details">
                                @Html.DisplayFor(model => model.ComputerName)
                            </dd>

                            <dt id="details">
                                @Html.DisplayNameFor(model => model.IPAddress)
                            </dt>

                            <dd id="details">
                                @Html.DisplayFor(model => model.IPAddress)
                            </dd>

                            <dt id="details">
                                @Html.DisplayNameFor(model => model.SerialNumber)
                            </dt>

                            <dd id="details">
                                @Html.DisplayFor(model => model.SerialNumber)
                            </dd>

                            <dt id="details">
                                @Html.DisplayNameFor(model => model.Make)
                            </dt>

                            <dd id="details">
                                @Html.DisplayFor(model => model.Make)
                            </dd>

                            <dt id="details">
                                @Html.DisplayNameFor(model => model.Model)
                            </dt>

                            <dd id="details">
                                @Html.DisplayFor(model => model.Model)
                            </dd>

                            <dt id="details">
                                @Html.DisplayNameFor(model => model.OS)
                            </dt>

                            <dd id="details">
                                @Html.DisplayFor(model => model.OS)
                            </dd>

                            <dt id="details">
                                @Html.DisplayNameFor(model => model.TotalMemory)
                            </dt>

                            <dd id="details">
                                @Html.DisplayFor(model => model.TotalMemory)
                            </dd>

                            <dt id="details">
                                @Html.DisplayNameFor(model => model.CPUSockets)
                            </dt>

                            <dd id="details">
                                @Html.DisplayFor(model => model.CPUSockets)
                            </dd>

                            <dt id="details">
                                @Html.DisplayNameFor(model => model.CPUCoresPerSocket)
                            </dt>

                            <dd id="details">
                                @Html.DisplayFor(model => model.CPUCoresPerSocket)
                            </dd>

                            <dt id="details">
                                @Html.DisplayNameFor(model => model.Drives)
                            </dt>

                            <dd id="details">
                                @Html.DisplayFor(model => model.Drives)
                            </dd>

                            <dt id="details">
                                @Html.DisplayNameFor(model => model.ProbeSiteName)
                            </dt>

                            <dd id="details">
                                @Html.DisplayFor(model => model.ProbeSiteName)
                            </dd>

                            <dt id="details">
                                @Html.DisplayNameFor(model => model.DateImported)
                            </dt>

                            <dd id="details">
                                @Html.DisplayFor(model => model.DateImported)
                            </dd>

                            <dt id="details">
                                @Html.DisplayNameFor(model => model.LastUpdated)
                            </dt>

                            <dd id="details">
                                @Html.DisplayFor(model => model.LastUpdated)
                            </dd>

                        </dl>
                    </div>
                    <p>
                        @Html.ActionLink("Back to List", "Index", new { id = 1 }, new { @class = "btn btn-primary" })
                    </p>
                </div>


            </div>
        </div>


    </div>



</body>
</html>
